{
  "cSpell.enabledFileTypes": {
    "markdown": true,
    "txt": true
  },
  // "cSpell.diagnosticLevel": ,
  // "workbench.colorCustomizations": {
  //   // will change the color of three dots to red
  //   "editorHint.foreground": "#ff0000",
  //   // will underline the entire word with dots in your chosen color
  //   "editorHint.border": "#00ff66"
  // },
  "cSpell.ignoreRandomStrings": true,
  "cSpell.language": [
    "es",
    "en"
  ],
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.fontVariations": true,
  "files.autoSave": "off",
  "code-runner.saveFileBeforeRun": true,
  "code-runner.runInTerminal": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.tabSize": 4,
    "editor.useTabStops": false,
    "editor.tabFocusMode": false,
  },
  "cSpell.words": [
    "calificacion",
    "CALORIAS",
    "cancion",
    "condicion",
    "elif",
    "fitness",
    "Ivonne",
    "kcal",
    "kilocalorias",
    "numeros",
    "operandos",
    "print",
    "randint",
    "reproduccion",
    "sublista",
    "sublistas",
    "tupla",
    "tuplas",
    "Tuplas"
  ],
  "CodeGPT.apiKey": "Ollama",
}